{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^10.0",
        "laravel/passport": "^11.0",
        "laravel/socialite": "^5.5",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^4.0",
        "league/commonmark": "^2.2",
        "myclabs/php-enum": "^1.7",
        "romanzipp/dto": "^2.3",
        "romanzipp/laravel-dto": "^0.0.15",
        "romanzipp/laravel-previously-deleted": "^1.3",
        "romanzipp/laravel-queue-monitor": "^2.1",
        "romanzipp/laravel-seo": "^2.3",
        "romanzipp/laravel-turnstile": "^1.1",
        "socialiteproviders/apple": "^5.3",
        "socialiteproviders/google": "^4.1",
        "socialiteproviders/twitter": "^4.1",
        "spatie/laravel-medialibrary": "^10.1"
    },
    "require-dev": {
        "spatie/laravel-ignition": "^2.0",
        "fakerphp/faker": "^1.16",
        "friendsofphp/php-cs-fixer": "^3.1.0",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^7.0",
        "phpstan/phpstan": "^1.0",
        "phpunit/phpunit": "^10.0",
        "romanzipp/laravel-env-normalizer": "^1.0.0",
        "romanzipp/laravel-model-doc": "^1.0.0",
        "romanzipp/php-cs-fixer-config": "^3.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/App",
            "Domain\\": "app/Domain",
            "Support\\": "app/Support",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "format": [
            "php-cs-fixer fix"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "repositories": {
        "nova": {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    }
}
